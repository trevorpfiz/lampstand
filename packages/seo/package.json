{
  "name": "@lamp/seo",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./json-ld": {
      "types": "./dist/json-ld.d.ts",
      "default": "./src/json-ld.ts"
    },
    "./metadata": {
      "types": "./dist/metadata.d.ts",
      "default": "./src/metadata.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
