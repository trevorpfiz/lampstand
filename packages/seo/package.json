    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "lodash.merge": "^4.6.2",
    "react": "catalog:react19",
    "schema-dts": "^1.1.2"
  },
  "devDependencies": {
    "@lamp/eslint-config": "workspace:*",
    "@lamp/prettier-config": "workspace:*",
    "@lamp/tsconfig": "workspace:*",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "catalog:",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "eslint": "catalog:",
    "next": "catalog:nextjs",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@lamp/prettier-config"
}
