{
  "name": "@lamp/observability",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./error": {
      "types": "./dist/error.d.ts",
      "default": "./src/error.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
