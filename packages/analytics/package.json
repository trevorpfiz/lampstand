    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@lamp/eslint-config": "workspace:*",
    "@lamp/prettier-config": "workspace:*",
    "@lamp/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
