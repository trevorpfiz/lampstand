{
  "name": "@lamp/feature-flags",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./access": {
      "types": "./dist/access.d.ts",
      "default": "./src/access.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@lamp/analytics": "workspace:*",
    "@lamp/supabase": "workspace:*",
    "@vercel/flags": "^2.6.3"
  },
  "peerDependencies": {
    "next": "catalog:nextjs"
  },
  "devDependencies": {
    "@lamp/eslint-config": "workspace:*",
    "@lamp/prettier-config": "workspace:*",
    "@lamp/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@lamp/prettier-config"
}
