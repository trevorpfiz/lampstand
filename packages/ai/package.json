{
  "name": "@lamp/ai",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    "./server": {
      "types": "./dist/clients/server.d.ts",
      "default": "./src/clients/server.ts"
    },
    "./client": {
      "types": "./dist/clients/client.d.ts",
      "default": "./src/clients/client.ts"
    },
    "./middleware": {
      "types": "./dist/clients/middleware.d.ts",
      "default": "./src/clients/middleware.ts"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./src/types/index.ts"
    },
    "./config/routes": {
      "types": "./dist/config/routes.d.ts",
      "default": "./src/config/routes.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.8",
    "@lamp/env": "workspace:*",
    "@lamp/logger": "workspace:*",
    "ai": "^4.0.18",
    "react": "catalog:react19",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.5.5"
  },
  "devDependencies": {
    "@lamp/eslint-config": "workspace:*",
    "@lamp/prettier-config": "workspace:*",
    "@lamp/tsconfig": "workspace:*",
    "dotenv-cli": "^7.4.4",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "next": ">=15.1.0"
  },
  "prettier": "@lamp/prettier-config"
}
