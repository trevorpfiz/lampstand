{
  "name": "@lamp/ai",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./react": {
      "types": "./dist/lib/react.d.ts",
      "default": "./src/lib/react.ts"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./src/components/index.ts"
    },
    "./models": {
      "types": "./dist/config/models.d.ts",
      "default": "./src/config/models.ts"
    },
    "./prompts": {
      "types": "./dist/config/prompts.d.ts",
      "default": "./src/config/prompts.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.10",
    "@lamp/env": "workspace:*",
    "@lamp/logger": "workspace:*",
    "ai": "catalog:",
    "react": "catalog:react19",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.5.5"
  },
  "devDependencies": {
    "@lamp/eslint-config": "workspace:*",
    "@lamp/prettier-config": "workspace:*",
    "@lamp/tsconfig": "workspace:*",
    "dotenv-cli": "^7.4.4",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "next": ">=15.1.0"
  },
  "prettier": "@lamp/prettier-config"
}
